5 import java.util.Scanner;


/**
 * Created by nikitinmd.18 on 21.11.2016.
 */

public class add {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int sum = 0;
        int k = 1;
        for (int i=0; i<a; i++){
            int b = sc.nextInt();
            if ((k == 2)||(k == 5)||(k == 8)){
                sum+=b;
            }
            k++;
        }
        System.out.println(sum);
    }
}

6 import java.util.Scanner;

/**
 * Created by nikitinmd.18 on 21.11.2016.
 */

public class add {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int i = 0;
        do {
            a = a/ 10;
            i++;
        }
        while (a > 0);
        System.out.println(i);
    }
}
7 import java.util.Scanner;

/**
 * Created by nikitinmd.18 on 21.11.2016.
 */

public class add {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int sum = 0;
        int i = 0;
        while (i<a){
            sum += b;
            i++;
        }
        System.out.println(sum);
    }
}

