9  import java.util.Scanner;
public class msc {
    public static void main(String[] arge){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();

        for (int i = 0; i < n; i++)
        {
            for (int j = 0; j < n - i - 1; j++)
            {
                System.out.print(' ');
            }
            for (int j = 0; j < i+1; j++)
            {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
10  import java.util.Scanner;
public class msc {
    public static void main(String[] arge) {
 Scanner sc = new Scanner(System.in);

        String str = sc.nextLine();
        char ch = str.toCharArray()[0];
        int N = str.toCharArray()[1] - 48;
        if (str.length() > 2)
        {
            N = N * 10;
            N = N + str.toCharArray()[2] - 48;
        }

        for (int i = ch; i < ch + N; i++)
        {
            if (i == 'Z')
            {
                System.out.print('Z');
                break;
            }
            else
            {
                System.out.print((char)i);
            }
        }
    }
}
