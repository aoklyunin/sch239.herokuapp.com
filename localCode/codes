5 import java.util.Scanner;
public class msc {
    public static void main(String[] arge) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int cnt = 0;
        for (int i = 0; i < n; i++){
            int a = sc.nextInt();
            if (i==1 || i == 4 || i == 7){
                cnt+= a;
            }
        }
        System.out.println(cnt);
    }
}
6 import java.util.Scanner;
public class msc {
    public static void main(String[] arge) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int cnt = 0;
        n = n * 10;

        for (int i = 0; n >= 10; i++) {
            n = n / 10;
            cnt++;
        }
        System.out.println(cnt);
    }
}


7 import java.util.Scanner;
public class msc {
    public static void main(String[] arge) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = sc.nextInt();
        int l = 0;
        for (int i = 0; i<2; i++) {
            l = m*n;
        }
        System.out.println(l);
    }
}

